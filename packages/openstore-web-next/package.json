{
  "name": "openstore-web-next",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run pot-file && astro dev --host",
    "start": "npm run pot-file && astro dev --host",
    "build": "npm run convert-po && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "sync": "astro sync",
    "convert-icons": "svgr --out-dir src/components/icons/ src/assets/icons/ && sed -i 's/#000/currentColor/g' src/components/icons/* && sed -i 's/gray/currentColor/g' src/components/icons/*",
    "convert-po": "bash -c 'for file in po/*.po; do LANG=$(basename \"$file\" .po); ./node_modules/.bin/i18next-conv -l \"$LANG\" -s \"po/$LANG.po\" -t \"src/locales/$LANG.json\"; done'",
    "pot-file": "node ./scripts/extract-strings.js"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/db": "^0.14.11",
    "@astrojs/node": "^9.1.3",
    "@astrojs/preact": "^4.0.9",
    "@astrojs/sitemap": "^3.3.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@libsql/client": "^0.15.3",
    "@nanostores/preact": "^1.0.0",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-switch": "^1.1.4",
    "@sentry/astro": "^9.12.0",
    "@tailwindcss/vite": "^4.1.4",
    "astro": "^5.6.2",
    "astro-capo": "^0.0.1",
    "astro-i18n-aut": "^0.7.3",
    "astro-seo": "^0.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "i18next": "^24.2.3",
    "i18next-conv": "^15.1.1",
    "lucide-react": "^0.488.0",
    "nanostores": "^1.0.1",
    "preact": "^10.26.5",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.4",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.8.3",
    "use-debounce": "^10.0.4",
    "zod-to-json-schema": "^3.24.5"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  },
  "volta": {
    "node": "22.14.0"
  },
  "devDependencies": {
    "@svgr/cli": "^8.1.0"
  }
}
